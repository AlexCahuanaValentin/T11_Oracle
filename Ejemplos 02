//ejemplo de Minimo
SELECT MIN(precio_mensual) AS plan_minimo
FROM planes_internet;

//ejemplo de Maximo
SELECT MAX(precio_mensual) AS plan_máximo
FROM planes_internet;

/*ejemplo de STDDEV En este ejemplo, estamos calculando la 
desviación estándar de los salarios en la tabla "employees". 
La función STDDEV calculará la desviación estándar de los
salarios y devolverá el resultado con el alias "desviación_estándar_salario".
*/
SELECT STDDEV(precio_mensual) AS desviación_estándar_salario
FROM planes_internet;

//ejemplo de la varianza
SELECT VARIANCE(precio_mensual) AS varianza_salario
FROM planes_internet;
//ejemplo de variance de las ventas
SELECT VARIANCE(monto) AS varianza_ventas
FROM ventas;
